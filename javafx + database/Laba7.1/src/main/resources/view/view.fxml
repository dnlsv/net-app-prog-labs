<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>

<!--<?import javafx.scene.web.*?>-->

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ViewController">
    <children>
        <TableView fx:id="productTable" layoutY="159.0" onMouseClicked="#tableAction" prefHeight="241.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="productID" prefWidth="48.0" text="id" />
                <TableColumn fx:id="productName" prefWidth="159.0" text="Название" />
                <TableColumn fx:id="productCost" prefWidth="114.0" text="Стоимость" />
                <TableColumn fx:id="productQuantity" prefWidth="125.0" text="Количество" />
                <TableColumn fx:id="productYear" prefWidth="153.0" text="Год выпуска" />
            </columns>
        </TableView>
        <AnchorPane prefHeight="87.0" prefWidth="600.0" style="-fx-background-color: #2E3348;">
            <children>
                <Label layoutX="152.0" layoutY="23.0" prefHeight="41.0" prefWidth="297.0" text="Магазин электротоваров" textFill="WHITE">
                    <font>
                        <Font name="System Italic" size="24.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>
        <Button fx:id="addButton" layoutX="110.0" layoutY="107.0" mnemonicParsing="false" onAction="#addButtonAction" text="Добавить" />
        <Button fx:id="editButton" layoutX="239.0" layoutY="107.0" mnemonicParsing="false" onAction="#editButtonAction" text="Редактировать" />
        <Button fx:id="deleteButton" layoutX="406.0" layoutY="107.0" mnemonicParsing="false" onAction="#deleteButtonAction" text="Удалить" />
    </children>
</AnchorPane>
